<?xml version="1.0" encoding="UTF-8"?>

<!-- You don't need to edit this file as it will be recreated each time a test runs -->

<phpunit 
    bootstrap               = ""
    backupGlobals           = "true"
    backupStaticAttributes  = "false"
    strict                  = "true"
    verbose                 = "true"
    colors                  = "true"
    >

  <testsuites>
  </testsuites>


  <!-- This is required for VPU to work correctly -->
  <listeners>
    <listener class="PHPUnit_Util_Log_JSON"></listener>
  </listeners>

</phpunit>